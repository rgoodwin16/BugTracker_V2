

<!DOCTYPE html>
<!--[if lt IE 7]>  <html class="lt-ie7"> <![endif]-->
<!--[if IE 7]>     <html class="lt-ie8"> <![endif]-->
<!--[if IE 8]>     <html class="lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html>
<!--<![endif]-->
<head>
    
    <!-- ==============================================
         Title and Meta Tags
         =============================================== -->
        
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>BugTracker - by Ray Goodwin</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- ==============================================
         Favicons
         ============================================== -->
    <link rel="icon" type="image/png" href="~/images/icon.png">

    <!-- ==============================================
         Google Fonts
         ============================================== -->

    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,300,500,700,900' rel='stylesheet' type='text/css'>

    <!-- ==============================================
         CSS
         =============================================== -->

    @Styles.Render("~/Content/css")
    <link href="~/Content/_Con Theme CSS/nanoscroller.css" rel="stylesheet" /><!-- nanoScroller -->
    <link href="~/css/font-awesome.min.css" rel="stylesheet" /><!-- Font Awesome -->
    <link href="~/Content/_Con Theme CSS/material-design-icons.css" rel="stylesheet" /><!-- Material Design Icons -->
    <link href="~/Content/_Con Theme CSS/ionicons.min.css" rel="stylesheet" /><!-- IonIcons -->
    <link href="~/Content/_Con Theme CSS/weather-icons.min.css" rel="stylesheet" /><!-- WeatherIcons -->
    <link href="~/Content/_Con Theme CSS/rickshaw.min.css" rel="stylesheet" /><!-- Rickshaw -->
    <link href="~/Content/_Con Theme CSS/jquery-jvectormap.css" rel="stylesheet" /><!-- jvectormap -->
    <link href="~/Content/_Con Theme CSS/prettify.css" rel="stylesheet" /><!-- Google Prettify -->
    <link href="~/Content/_Con Theme CSS/_con.css" rel="stylesheet" /><!-- Main -->
    <link href="~/Scripts/select2/css/select2.min.css" rel="stylesheet" /><!-- Select 2-->

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <!--
      Top Navbar
        Options:
          .navbar-dark - dark color scheme
          .navbar-static - static navbar
          .navbar-under - under sidebar
    -->
    <nav class="navbar-top">
        <div class="nav-wrapper">
            <!-- Sidebar toggle -->
            <a href="#" class="yay-toggle">
                <div class="burg1"></div>
                <div class="burg2"></div>
                <div class="burg3"></div>
            </a>
            <!-- Sidebar toggle -->
            <!-- Logo -->
            <a href="@Url.Action("Index","DashBoard")" class="brand-logo">
                <img src="~/images/logo.png" alt="BugTracker">
            </a>
            <!-- /Logo -->
            <!-- Menu -->
            <ul>

                <li class="user">
                    <a class="dropdown-button" href="#!" data-activates="user-dropdown">
                        
                        @User.Identity.Name
                        
                        <i class="mdi-navigation-expand-more right"></i>
                    </a>
                    <ul id="user-dropdown" class="dropdown-content">
                        
                            @Html.Partial("_LoginPartial")
                        
                    </ul>
                </li>
            </ul>
            <!-- /Menu -->
        </div>
    </nav>
    <!-- /Top Navbar -->

    <!--
      Yay Sidebar
      Options [you can use all of theme classnames]:
        .yay-hide-to-small         - no hide menu, just set it small with big icons
        .yay-static                - stop using fixed sidebar (will scroll with content)
        .yay-gestures              - to show and hide menu using gesture swipes
        .yay-light                 - light color scheme
        .yay-hide-on-content-click - hide menu on content click

      Effects [you can use one of these classnames]:
        .yay-overlay  - overlay content
        .yay-push     - push content to right
        .yay-shrink   - shrink content width
    -->
    <aside class="yaybar yay-shrink yay-hide-to-small yay-gestures yay-light yay-static">
        <div class="top">
            <div>
                <!-- Sidebar toggle -->
                <a href="#" class="yay-toggle">
                    <div class="burg1"></div>
                    <div class="burg2"></div>
                    <div class="burg3"></div>
                </a>
                <!-- Sidebar toggle -->
                <!-- Logo -->
                <a href="@Url.Action("Index","DashBoard")" class="brand-logo">
                    <img src="assets/_con/images/logo-white.png" alt="">
                </a>
                <!-- /Logo -->
            </div>
        </div>

        <div class="nano">
            <div class="nano-content">
                <ul>
                    
                    <li class="label">Menu</li>
                    
                    <!-- Dashboard -->

                    <li>
                        <a href="@Url.Action("Index","Dashboard")" class="waves-effect waves-blue"><i class="fa fa-dashboard"></i> Dashboard</a>
                        
                    </li>
                   
                    <!-- User Profile -->

                    <li class="label">User Menu</li>

                    <li>
                        <a href="@Url.Action("Index","Manage")" class="waves-effect waves-blue"><i class="fa fa-user"></i> Profile</a>
                    </li>
                    
                    @if(User.IsInRole("Admin"))
                    {
                        <li>
                            <a href="@Url.Action("Users", "Admin")" class="waves-effect waves-blue"><i class="fa fa-users"></i> User Roles</a>
                        </li>
                    }

                    
                    
                </ul>
            </div>
        </div>
    </aside>
    <!-- /Yay Sidebar -->

    

     @RenderBody()

   




    <footer>
        &copy; 2015 Bug<strong>Tracker</strong>.  Developed by: <a href="https://rgoodwin.azurewebsites.net">Ray Goodwin</a>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    

    <!-- jQuery RAF (improved animation performance) -->
    <script src="~/Scripts/jqueryRAF/jquery.requestAnimationFrame.min.js"></script>

    <!-- nanoScroller -->
    <script src="~/Scripts/nanoScroller/jquery.nanoscroller.min.js"></script>

    <!-- Materialize -->
    <script src="~/Scripts/materialize/materialize.min.js"></script>

    <!-- Simple Weather -->
    <script src="~/Scripts/simpleWeather/jquery.simpleWeather.min.js"></script>

    <!-- Sparkline -->
    <script src="~/Scripts/sparkline/jquery.sparkline.min.js"></script>

    <!-- Flot -->
    <script src="~/Scripts/flot/jquery.flot.min.js"></script>
    <script src="~/Scripts/flot/jquery.flot.time.min.js"></script>
    <script src="~/Scripts/flot/jquery.flot.pie.min.js"></script>
    <script src="~/Scripts/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/Scripts/flot/jquery.flot.categories.min.js"></script>

    <!-- d3 -->
    <script src="~/Scripts/d3/d3.min.js"></script>

    <!-- Rickshaw -->
    <script src="~/Scripts/rickshaw/rickshaw.min.js"></script>

    <!-- jvectormap -->
    <script src="~/Scripts/jquery-jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/Scripts/jquery-jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Scripts/jquery-jvectormap/gdp-data.js"></script>

    <!-- Sortable -->
    <script src="~/Scripts/sortable/Sortable.min.js"></script>

    <!-- Main -->
    <script src="~/Scripts/_con/_con.min.js"></script>

    <!-- Google Prettify -->
    <script src="~/Scripts/google-code-prettify/prettify.js"></script>

    <!-- datatables -->
    <script src="//cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js"></script>
    


    
    @RenderSection("scripts", required: false)
</body>
</html>
