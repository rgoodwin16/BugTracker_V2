                        <button class="btn btn-default" type="button" onclick="location.href='@Url.Action("Register","Account")'">Register</button>
                    </div>
                    
                </div>

                <div class="col-lg-4">
                    <h3 class="centered"><i class="glyphicon glyphicon-log-in"></i> Use existing account.</h3>
                    
                    <hr />
                    
                    <div class="btn-group-lg centered">
                        <button class="btn btn-default" type="button" onclick="location.href='@Url.Action("Login","Account")'">Login</button>
                    </div>
                </div>

                <div class="col-lg-4">
                                      

                    <div class="btn-group-lg centered" role="group">
                       @Html.Partial("~/Views/Account/_LandingPageExternalLogin.cshtml", new ExternalLoginListViewModel { ReturnUrl = ViewBag.ReturnUrl })
