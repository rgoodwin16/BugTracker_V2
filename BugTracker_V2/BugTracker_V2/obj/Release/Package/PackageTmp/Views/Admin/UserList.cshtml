@model BugTracker_V2.Models.UserRoles
@{
    ViewBag.Title = "UserList";
}


<section class="content-wrap ecommerce-products">
    
    <div class="card">
        <div class="title">
            <h5>Users</h5>
        </div><!-- /title -->
    </div><!-- /card -->
    
    <div class="content">
        
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Role Name</th>
                    <th>Current Users in Role</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var role in Model.RoleList)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => role.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => role.Users)
                        </td>
                      
                    </tr>
                }


            </tbody><!-- /tbody -->
        </table><!-- /table -->

    </div><!-- /content -->

</section>


